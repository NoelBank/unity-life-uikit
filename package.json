{
  "name": "unity-life-uikit",
  "version": "0.0.1",
  "description": "Uikit for Unity Life GTA 5 Project writen in React for visualization we use Storybook",
  "main": "./src/uikit/index.ts",
  "repository": "https://github.com/NoelBank/unity-life-uikit.git",
  "author": "Noel Bank <info@nbank.dev>",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/preset-react": "7.12.5",
    "@babel/preset-typescript": "7.12.1",
    "@storybook/addon-actions": "6.0.28",
    "@storybook/addon-essentials": "6.0.28",
    "@storybook/addon-links": "6.0.28",
    "@storybook/react": "6.0.28",
    "@types/react": "16.9.56",
    "babel-loader": "8.1.0",
    "babel-plugin-universal-import": "3.1.3",
    "concurrently": "5.3.0",
    "css-loader": "5.0.1",
    "file-loader": "6.2.0",
    "node-sass": "5.0.0",
    "plop": "2.7.4",
    "prettier": "2.1.2",
    "sass-loader": "10.0.5",
    "style-loader": "2.0.0",
    "typed-scss-modules": "3.3.0",
    "typescript": "4.0.5",
    "ts-node": "9.0.0",
    "tsconfig-paths": "3.9.0"
  },
  "scripts": {
    "generate-component": "plop component --plopfile plop/plopfile.js",
    "gc": "yarn generate-component",
    "scss-types": "tsm src/**/*.module.scss  --logLevel error --exportType default",
    "start:storybook": "start-storybook -p 2018",
    "start": "concurrently \"yarn scss-types -w\" \"yarn start:storybook\" --names 'types,storybook'",
    "format:all": "prettier --write \"{src/**/*.@(js|jsx|ts|tsx|json|scss),./*.@(js|json)}\""
  }
}
